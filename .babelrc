{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": "false",
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-modules-commonjs",
    ["@babel/plugin-proposal-class-properties",{"loose": true}]
  ]
}
