{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 0
  },
  "globals": {
    "DEBUG": false,
    "define": true
  }
}
